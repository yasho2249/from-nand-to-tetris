/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=selbar);
    And(a=selbar, b=a, out=temp1);
    And(a=sel, b=b, out=temp2);
    Or(a=temp1, b=temp2, out=out);
}